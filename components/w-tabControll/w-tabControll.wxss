.tabControll{
  display: flex;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
}

.tabcon-item{
  flex: 1;
  text-align: center;
}

.active{
  color: #ff5777;
  border-bottom: 6rpx solid #ff5777;
}

.tabcon-item text{
  padding: 18rpx 12rpx;
}
