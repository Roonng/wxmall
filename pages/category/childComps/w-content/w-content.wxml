<!--pages/category/childComps/w-content/w-content.wxml-->
<scroll-view scroll-y class="content">
  <view class="con">
    <block wx:for="{{contentList}}" wx:key="{{index}}">
      <view class="contItem">
        <image src="{{item.image}}"/>
        <view>{{item.title}}</view>
      </view>
    </block>
  </view>
</scroll-view>